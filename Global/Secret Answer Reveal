var elderberry = document.createElement("p");
elderberry.style.color = "transparent";
document.body.appendChild(elderberry);
setInterval(function () {
  var jackfruit = Object.values(document.querySelector("#app > div > div"))[1]
    .children[1]["_owner"].stateNode.state.question;
  elderberry.innerText = jackfruit.correctAnswers;
});

let date = document.createElement("iframe");
document.body.append(date);
window.alert = date.contentWindow.alert.bind(window);
date.remove();

function guava(fig) {
  window[alert(fig)];
}
var mango = function (feijoa) {
  if (feijoa.key === "Control") {
    document.addEventListener("keydown", function (strawberry) {
      if (strawberry.key === "b") {
        elderberry.style.color = "Black";
        elderberry.style.fontSize = "x-large";
      }
    });
  }
};
document.body.onkeyup = function () {
  mango(event);
};
var cranberry = function (pomelo) {
  if (pomelo.key === "Control") {
    document.addEventListener("keyup", function (avocado) {
      if (avocado.key === "b") {
        elderberry.style.color = "transparent";
        elderberry.style.fontSize = "x-large";
      }
    });
  }
};
document.body.onkeydown = function () {
  cranberry(event);
};
